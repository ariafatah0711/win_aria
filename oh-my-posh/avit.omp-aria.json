{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\n",
          "type": "text"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#ffffff",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#C2C206",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#B5B50D",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "x{{ reason .Code }} ",
          "foreground": "#C94A16",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b",
            "windows": "\ue70f",
            "wsl": "",
            "wsl_separator": ""
          },
          "template": "{{ if .WSL }}{{ end }} {{.Icon}} ",
          "foreground": "#e83516",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue602",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
