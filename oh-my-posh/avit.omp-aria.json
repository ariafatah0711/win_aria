{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05 02-Jan"
          },
          "template": "{{ .CurrentDate | date .Format }} ",
          "foreground": "#00FFAA",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#ffffff",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#C2C206",
          "type": "git",
          "style": "plain"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#906cff",
          "template": "\uE235 {{ if .Venv }}({{ .Venv }}) {{ end }}{{ .Full }} ",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true,
            "cache_duration": "none"
          }
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#B5B50D",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "x{{ reason .Code }} ",
          "foreground": "#C94A16",
          "type": "status",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31b",
            "windows": "\ue70f",
            "cache_duration": "none",
            "wsl": "",
            "wsl_separator": ""
          },
          "template": "{{ if .WSL }}{{ end }} {{.Icon}} ",
          "foreground_templates": [
            "{{ if eq .OS \"windows\" }}#00A4EF{{ end }}",
            "{{ if eq .OS \"arch\" }}#1793D1{{ end }}",
            "{{ if eq .OS \"ubuntu\" }}#E95420{{ end }}",
            "{{ if eq .OS \"debian\" }}#A81D33{{ end }}",
            "{{ if eq .OS \"fedora\" }}#294172{{ end }}",
            "{{ if eq .OS \"manjaro\" }}#34BE5B{{ end }}",
            "{{ if eq .OS \"macos\" }}#999999{{ end }}",
            "{{ if eq .OS \"linux\" }}#FFDD00{{ end }}"
          ],
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue602",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
